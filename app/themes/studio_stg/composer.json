{
  "name": "roots/sage",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "WordPress starter theme with a modern development workflow",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": ">=8.1",
    "log1x/acf-composer": "^3.2",
    "log1x/navi": "^3.1",
    "log1x/poet": "^2.1",
    "log1x/sage-directives": "^2.0",
    "log1x/sage-svg": "^2.0",
    "roots/acorn": "^4.3",
    "roots/acorn-prettify": "^1.0",
    "spatie/laravel-ignition": "^2.8"
  },
  "require-dev": {
    "laravel/pint": "^1.18"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "acorn": {
      "providers": [
        "App\\Providers\\ThemeServiceProvider"
      ]
    }
  },
  "scripts": {
    "lint:fix": "./vendor/bin/pint",
    "lint:test": "./vendor/bin/pint --test"
  }
}
